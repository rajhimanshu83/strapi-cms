{
  "connection": "default",
  "collectionName": "articles",
  "info": {
    "name": "articles",
    "description": "blog"
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "content": {
      "required": true,
      "type": "text"
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "category": {
      "type": "string"
    },
    "tag": {
      "type": "string"
    }
  }
}